<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="#00000000"></solid>
            <stroke android:width="2dp" android:color="@color/c1_2" />
            <corners android:radius="@dimen/global_button_corner_radius"></corners>
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <solid android:color="#00000000"></solid>
            <stroke android:width="2dp" android:color="@color/c1_1_60" />
            <corners android:radius="@dimen/global_button_corner_radius"></corners>
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#00000000"></solid>
            <stroke android:width="2dp" android:color="@color/c1_1" />
            <corners android:radius="@dimen/global_button_corner_radius"></corners>
        </shape>
    </item>
</selector>